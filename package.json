{
    "name": "svg2png-loader",
    "description": "Webpack Loader to rasterize SVG vector into multiple PNG images for different pixel-ratios using PhantomJS",
    "version": "1.0.0",
    "license": "MIT",
    "main": "index.js",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack",
        "test": "jest",
        "example": "webpack --config example/development.js"
    },
    "keywords": [
        "webpack-loader",
        "svg",
        "png",
        "electron",
        "phantomjs"
    ],
    "engines": {
        "node": ">=8"
    },
    "author": {
        "name": "Ádám Székely",
        "url": "https://github.com/enteocode",
        "email": "stereotribe@hotmail.com"
    },
    "files": [
        "index.js",
        "index.d.ts"
    ],
    "jest": {
        "testEnvironment": "node",
        "preset": "ts-jest",
        "bail": true,
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "verbose": true
    },
    "peerDependencies": {
        "imagemin-optipng": "*",
        "webpack": ">=2"
    },
    "devDependencies": {
        "@types/jest": "^25.2.1",
        "@types/loader-utils": "^1.1.3",
        "@types/memory-fs": "^0.3.2",
        "@types/svg2png": "^4.1.0",
        "@types/terser-webpack-plugin": "^2.2.0",
        "@types/webpack-env": "^1.15.2",
        "@types/webpack-virtual-modules": "^0.1.0",
        "@typescript-eslint/parser": "^2.30.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "eslint-loader": "^4.0.2",
        "imagemin-optipng": "^7.1.0",
        "jest": "^25.5.1",
        "memory-fs": "^0.5.0",
        "terser-webpack-plugin": "^2.3.6",
        "ts-jest": "^25.4.0",
        "ts-loader": "^7.0.1",
        "typescript": "^3.8.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-virtual-modules": "^0.2.2"
    },
    "dependencies": {
        "loader-utils": "^2.0.0",
        "svg2png": "^4.1.1"
    },
    "bugs": {
        "url": "https://github.com/enteocode/svg2png-loader/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enteocode/svg2png-loader.git"
    }
}
